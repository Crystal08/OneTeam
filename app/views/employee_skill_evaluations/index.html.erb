<table>
  <tr>
    <th>Response</th>
    <th>Skill</th>
    <th>Assigned skill level</th>
    <th>Skill experience points</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @employee_skill_evaluations.each do |employee_skill_evaluation| %>
  <tr>
    <td><%= employee_skill_evaluation.response_id %></td>
    <td><%= employee_skill_evaluation.skill_id %></td>
    <td><%= employee_skill_evaluation.assigned_skill_level %></td>
    <td><%= employee_skill_evaluation.skill_experience_points %></td>
    <td><%= link_to 'Show', employee_skill_evaluation %></td>
    <td><%= link_to 'Edit', edit_employee_skill_evaluation_path(employee_skill_evaluation) %></td>
    <td><%= link_to 'Destroy', employee_skill_evaluation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
